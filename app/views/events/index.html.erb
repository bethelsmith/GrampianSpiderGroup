<p>
  <% if current_user.admin? %>
  <%= link_to 'Create a New Event', new_event_path(@event) %>
  <% end %>
<p>

<h1>All Events</h1>
<div class= "search"?
<%= form_tag events_path, :method => :get do %>
  <div id = "box", class="field">
    Search events:
    <%= text_field_tag :search, params[:search] %>
  </div>
  <div id= "button", class="actions">
    <%= submit_tag "Search" %>
  </div>
</div>
<% end %>

<div class= "map">
  <%= gmaps4rails(@json) %>
</div>

</br>
<%= will_paginate %>

<ul class="events">
  <%= render @events %>
</ul>
</br>

<%= will_paginate %>
