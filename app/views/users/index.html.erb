<h1>All users</h1>
<div class= "search"?
<%= form_tag events_path, :method => :get do %>
  <div id = "box", class="field">
    Search users:
    <%= text_field_tag :search, params[:search] %>
  </div>
  <div id= "button", class="actions">
    <%= submit_tag "Search" %>
  </div>
</div>
<% end %>
</br>
<%= will_paginate %>

<ul class="users">
  <%= render @users %>
</ul>

<%= will_paginate %>